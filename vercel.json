{
  "functions": {
    "app/api/images/[country]/route.ts": {
      "maxDuration": 30,
      "excludeFiles": "public/**"
    }
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
